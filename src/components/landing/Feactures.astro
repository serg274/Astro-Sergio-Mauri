---
import Feactures from "../../config/Feactures.json";
---

<h1 class="text-center font-bold text-5xl mt-10 py-5">Feacture</h1>

<div class="grid md:grid-cols-2 p-2 gap-4">
  {
    Feactures.map((Feactures) => (
      <div class="bg-slate-800 p-10 rounded-xl ">
        <h2 class="text-2xl my-4 font-bold">{Feactures.title}</h2>
        <p>{Feactures.description}</p>
      </div>
    ))
  }
</div>
